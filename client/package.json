{
  "name": "sengo",
  "version": "1.0.0",
  "description": "An open source clean room implementation of a useful subset of the MongoDb client for node.js that uses AWS S3 as a backend location for documents.",
  "type": "commonjs",
  "main": "./build-cjs/index.js",
  "exports": {
    ".": {
      "require": "./build-cjs/index.js"
    }
  },
  "types": "./build-cjs/types/index.d.ts",
  "scripts": {
    "build": "tsc --project tsconfig.json && node scripts/dist.cjs",
    "test": "vitest run",
    "test:ui": "vitest",
    "clean": "rimraf build-cjs dist *.tsbuildinfo ./src/**/*.js ./src/**/*.test.ts src/testutils",
    "verify": "npm run clean && npm run build && npm test"
  },
  "keywords": [],
  "author": "Avery Regier <avery.regier@gmail.com> (https://github.com/AveryRegier)",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/chance": "^1.1.6",
    "@types/node": "^24.0.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "aws-sdk-client-mock": "^4.1.0",
    "chance": "^1.1.13",
    "rimraf": "^3.0.2",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.839.0",
    "bson": "^6.10.4",
    "semver": "^7.7.2"
  }
}
